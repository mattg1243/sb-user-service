{
  "name": "user",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/app.js",
  "scripts": {
    "build": "tsc",
    "prodstart": "node ./dist/app.js",
    "devstart": "nodemon ./src/app.ts",
    "protogen": "cd proto && buf generate",
    "genscript": "cd proto && zsh build.sh",
    "dockerstart": "docker run --rm -it --env-file ./.env -p 8080:8080/tcp sb-user:latest",
    "migrate": "rm -rf dist && npm run build && typeorm-ts-node-commonjs migration:generate ./src/migrations/addedEntity -d ./src/database/dataSource.ts",
    "db:push": "rm -rf dist && npm run build && typeorm-ts-node-commonjs migration:run -d ./src/database/dataSource.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "google-protobuf": "^3.21.2",
    "grpc-tools": "^1.11.3",
    "pg": "^8.8.0",
    "ts-proto": "^1.133.0",
    "typeorm": "^0.3.10",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/google-protobuf": "^3.15.6",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4"
  }
}
